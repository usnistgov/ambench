<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified">
  <!-- base types related to Resource-s, i.e. entities represented in an AMBench CDCS instance as document with PID assigned 
    etc. -->
  <xs:complexType name="AMResource" abstract="true">
    <xs:annotation>
      <xs:documentation>An XML schema type which represents a generic resource in the AM Bench
      project (https://www.nist.gov/ambench) such as samples and measurements.  
      In the NIST CDCS database, an AM Bench resource is represented as an XML document 
      with a persistent identifier (PID) assigned by the database.  
      In the schema, AMResource is the ultimate 
      base type for other more explicit AMBench documents 
      such as documents for AMBuildPlates, AMPowder, AMRadiography, etc.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" minOccurs="1" maxOccurs="1" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Name</label>
          </xs:appinfo>
          <xs:documentation>The name of a resource. It is used as an identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Description</label>
          </xs:appinfo>
          <xs:documentation>The description of a resource</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="documentation" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Documentation</label>
          </xs:appinfo>
          <xs:documentation>The URI of a document which contains an extended 
          description of a resource.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="primaryContact" type="Person" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Primary contact</label>
          </xs:appinfo>
          <xs:documentation>The primary contact for the content of a resource</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="contributor" type="Contributor" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Contributor</label>
          </xs:appinfo>
          <xs:documentation>The contributors to the content of a resource</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="note" type="Note" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Note</label>
          </xs:appinfo>
          <xs:documentation>Any information, usually in text or image files, 
          related to a resource which is not included in the elements of 'AMResource'.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="altName" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Alternate Name</label>
          </xs:appinfo>
          <xs:documentation>Any alternate name like an acronym or nickname of a resource.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UUID" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <label>UUID</label>
          </xs:appinfo>
          <xs:documentation>A universal unique identifier of a resource.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="identifier" type="identifier" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <label>Identifier</label>
          </xs:appinfo>
          <xs:documentation>Any identifiers which are associated with a resource 
          other than its name, alternate names, PID, or UUID</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="journalPublication" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Journal publication</label>
          </xs:appinfo>
          <xs:documentation>The list of publications resulting from a resource. 
          for example, the publication 10.1016/j.procir.2020.09.135 resulted 
          from the radiography measurements in the AM Bench project.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="referencePublication" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Reference publication</label>
          </xs:appinfo>
          <xs:documentation>The list of any reference publications relevant to 
          a resource other than those listed in journalPublication of a resource.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!-- Standard name and link are mapped to title, and url -->
      <xs:element name="relatedStandard" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Related standard</label>
          </xs:appinfo>
          <xs:documentation>The list of standards related to a resource. 
          For example, the standards with which mechanical testing measurements 
          comply.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AMActivity" abstract="true">
    <xs:annotation>
      <xs:documentation>It is a base type for any XML schema types for 
      any AM Bench resource representing an activity.  It extends 'AMResource'. Some examples 
      of activities in the AM Bench project are procurement of powder, various build processes,  
      measuring the composition of samples,
      laser absorptivity measurements, etc.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="AMResource">
        <xs:sequence>
          <xs:element name="startDate" minOccurs="0" maxOccurs="1" type="xs:dateTime">
            <xs:annotation>
              <xs:appinfo>
                <label>Start date</label>
              </xs:appinfo>
              <xs:documentation>A date or a date + time that an activity started</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="completeDate" minOccurs="0" maxOccurs="1" type="xs:dateTime">
            <xs:annotation>
              <xs:appinfo>
                <label>Complete date</label>
              </xs:appinfo>
              <xs:documentation>A date or a date + time that an activity is completed</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="identifier">
    <xs:annotation>
      <xs:documentation>An XML schema type which represents an identifier of any object to which
      identifier(s) can be assigned.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="id" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Identifier</label>
          </xs:appinfo>
          <xs:documentation>The value of an identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="type" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Type</label>
          </xs:appinfo>
          <xs:documentation>The type of an identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Field">
    <xs:annotation>
      <xs:documentation>An XML schema type which represents a member in the definition 
      of 'ObjectType'. It consists of a 'name' and 'value' pair, where the type of a value can 
      be a string, 'physical-quantity-type', 'DigitalArtifact' or 'ObjectType'.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Name</label>
          </xs:appinfo>
          <xs:documentation>The name of a field</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="value" type="xs:string">
          <xs:annotation>
            <xs:appinfo>
              <label>Value</label>
            </xs:appinfo>
            <xs:documentation>The value of a field whose type is string</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="quantity" type="physical-quantity-type">
          <xs:annotation>
            <xs:appinfo>
              <label>Quantity</label>
            </xs:appinfo>
            <xs:documentation>The value field of type 'physical-quantity-type'</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="object" type="ObjectType">
          <xs:annotation>
            <xs:appinfo>
              <label>Object</label>
            </xs:appinfo>
            <xs:documentation>The value field of type 'ObjectType'</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="digitalArtifact" type="DigitalArtifact">
          <xs:annotation>
            <xs:appinfo>
              <label>Digital artifact</label>
            </xs:appinfo>
            <xs:documentation>The value field of type 'DigitalArtifact'</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Description</label>
          </xs:appinfo>
          <xs:documentation>The description of a field</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="documentationLink" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Documentation</label>
          </xs:appinfo>
          <xs:documentation>The URI of a documentation related to a field</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ObjectType">
    <xs:annotation>
      <xs:documentation>An XML schema type which enables the declaration of a 
      structured data type. It is similar to a class in object oriented languages 
      or 'complexType' in XML schema. In general, it consists of a group of fields
      where the type of each attribute can be a primitive data type or another 'ObjectType'.
      For example, an XML element ‘specimenMetadata’ defined in the AM Bench measurement schema
        is a type of ‘ObjectType’ since metadata specific to any given specimen 
        can require arbitrary fields.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Name</label>
          </xs:appinfo>
          <xs:documentation>The name of an object type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Description</label>
          </xs:appinfo>
          <xs:documentation>The description of an object type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="field" type="Field" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Field</label>
          </xs:appinfo>
          <xs:documentation>A member of an object type</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="ArtifactType">
    <xs:annotation>
      <xs:documentation>The enumeration of the artifact types that are 
      defined in the AM Bench project</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="AMBuildPlate">
        <xs:annotation>
          <xs:documentation>AM Bench Build Plate</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AMBuildPart">
        <xs:annotation>
          <xs:documentation>AM Bench Build Part</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AMBSpecimen">
        <xs:annotation>
          <xs:documentation>AM Bench Specimen</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AMPowder">
        <xs:annotation>
          <xs:documentation>AM Bench Powder</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Material">
        <xs:annotation>
          <xs:documentation>Material</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="MaterialInfo">
    <xs:annotation>
      <xs:documentation>An XML schema type which describes an ultimate source material 
      of a physical artifact produced in the AM Bench project.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="materialClass" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Material class</label>
          </xs:appinfo>
          <xs:documentation>Material class.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="sourceMaterialId" type="xs:anyURI" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Source material ID</label>
          </xs:appinfo>
          <xs:documentation>The name of a source material</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- UTILITIES -->
  <xs:complexType name="Note">
    <xs:annotation>
      <xs:documentation>An XML schema type which represents any supplementary information relevant to  
      'AMResource' which is not among the elements of 'AMResource' or its extensions</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Title</label>
          </xs:appinfo>
          <xs:documentation>Title of a note</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="date" type="xs:dateTime" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Creation date</label>
          </xs:appinfo>
          <xs:documentation>Date when a note is created</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="text" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Text</label>
          </xs:appinfo>
          <xs:documentation>The text of a note.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="digitalArtifact" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Digital artifact</label>
          </xs:appinfo>
          <xs:documentation>Any file, folder or database of a note.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DigitalArtifact">
    <xs:annotation>
      <xs:documentation>An XML schema type which represents a digital object (e.g., file, folder 
      or database) created as a result of activities or relevant to a resource in the AM Bench project. 
      Examples are access URLs to various raw data or processed data from measurements 
      in the AM Bench project.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="identifier" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <label>Identifier</label>
          </xs:appinfo>
          <xs:documentation>An identifier of a digital artifact including its name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DOI" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <label>DOI</label>
          </xs:appinfo>
          <xs:documentation>DOI (Digital object identifier) of a digital artifact</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="title" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Title</label>
          </xs:appinfo>
          <xs:documentation>Title of a digital artifact</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="description" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Description</label>
          </xs:appinfo>
          <xs:documentation>Description of a digital artifact</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="type" type="DigitalArtifactType">
        <xs:annotation>
          <xs:appinfo>
            <label>Type</label>
          </xs:appinfo>
          <xs:documentation>Type of a digital artifact. The allowed values are 
          file, folder, and database. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="format" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Format</label>
          </xs:appinfo>
          <xs:documentation>Data format of a digital artifact. Examples are
           pdf, jpeg, png, and etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="comment" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Comment</label>
          </xs:appinfo>
          <xs:documentation>Any supplementary comment on a digital artifact</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="accessURL" type="xs:anyURI">
        <xs:annotation>
          <xs:appinfo>
            <label>Download URL</label>
          </xs:appinfo>
          <xs:documentation>A URL which provides a direct access to a digital artifact.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="DigitalArtifactType">
    <xs:annotation>
      <xs:documentation>The enumeration of digital artifact types</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="file">
        <xs:annotation>
          <xs:documentation>File</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="folder">
        <xs:annotation>
          <xs:documentation>Folder</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="database">
        <xs:annotation>
          <xs:documentation>Database</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="AMBlobReference">
    <xs:annotation>
      <xs:documentation>
        A reference to an AMBlob document. 
        It contains the handle and the checksum of the actual blob in the AMBlob document.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="handle" type="xs:anyURI" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Handle</label>
          </xs:appinfo>
          <xs:documentation>
          The CDCS handle of the blob in a referenced AMBlob document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="checksum" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Checksum</label>
          </xs:appinfo>
          <xs:documentation>
            The MD5 checksum of the referenced blob bytes. This is a unique 
          value which can used as an identifier of the AMBlob representing the blob.
          Its value can be used to check whether the blob was 
          already uploaded in CDCS or not.          
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Description</label>
          </xs:appinfo>
          <xs:documentation>
            Description of the blob in a referenced AMBlob document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Blob">
    <xs:annotation>
      <xs:documentation>A wrapper providing extra metadata for a blob  
      in the CDCS database. The blobs are typically images which contain diagrams, or pictures which illustrate 
        specimen measurement geometry, build plates design, processing steps, 
        and others. </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="handle" type="xs:anyURI" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Handle</label>
          </xs:appinfo>
          <xs:documentation> 
            The CDCS handle of the wrapped blob in the database.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="checksum" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Checksum</label>
          </xs:appinfo>
          <xs:documentation>The MD5 checksum of the blob bytes. This is a unique 
          value which can used as an identifier of the AMBlob representing the blob.
          Its value can be used to check whether the blob was 
          already uploaded in CDCS or not. 
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cdcsPID" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>CDCS PID</label>
          </xs:appinfo>
          <xs:documentation>The PID assigned to the blob by CDCS. 
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="format" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Format</label>
          </xs:appinfo>
          <xs:documentation>The format of the blob. For example, png, jpeg, and etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Person">
    <xs:annotation>
      <xs:documentation>A base entity which represents a person.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="name" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Name</label>
          </xs:appinfo>
          <xs:documentation>Name of a person</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="email" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Email</label>
          </xs:appinfo>
          <xs:documentation>Email address of a person</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="orcidID" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>ORCID ID</label>
          </xs:appinfo>
          <xs:documentation>ORCID ID of a person</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="location" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Location</label>
          </xs:appinfo>
          <xs:documentation>Work location of a person</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="phone" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Phone number</label>
          </xs:appinfo>
          <xs:documentation>Phone number of a person</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="affiliation" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Affiliation</label>
          </xs:appinfo>
          <xs:documentation>Affiliation of a person</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Contributor">
    <xs:annotation>
      <xs:documentation>A type representing a person who contributes to a resource 
       in the AM Bench project</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="person" type="Person" minOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Person</label>
          </xs:appinfo>
          <xs:documentation>A reference to a person.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="role" type="xs:string" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>Role</label>
          </xs:appinfo>
          <xs:documentation>A role that a person plays in various aspects of a resource. 
            For example, primary contact, owner, custodian, responsible party, and etc.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Instrument">
    <xs:annotation>
      <xs:documentation>An XML schema type which describes an instrument used in an
      AM Bench measurement. It also includes the metadata for its configuration and usage
      in a measurement</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="AMResource">
        <xs:sequence>
