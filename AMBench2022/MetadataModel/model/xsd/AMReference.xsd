      <xs:enumeration value="hours" />
      <xs:enumeration value="minutes" />
      <xs:enumeration value="seconds" />
      <xs:enumeration value="milliseconds" />
      <xs:enumeration value="microseconds" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LengthUnit">
    <xs:restriction base="xs:string">
      <xs:enumeration value="picometer" />
      <xs:enumeration value="um" />
      <xs:enumeration value="mm" />
      <xs:enumeration value="cm" />
      <xs:enumeration value="m" />

    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ConstituentQuantityUnit">
    <xs:restriction base="xs:string">
      <xs:enumeration value="mass fraction" />
      <xs:enumeration value="mass percent" />
      <xs:enumeration value="mole fraction" />
      <xs:enumeration value="mole percent" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="FlowSpeedUnits">
    <xs:restriction base="xs:string">
      <xs:enumeration value="m/s" />
      <xs:enumeration value="mm/s" />
      <xs:enumeration value="other" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RotationAngleUnit">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Degrees" />
    </xs:restriction>
  </xs:simpleType>

