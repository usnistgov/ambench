      <xs:enumeration value="Stress-Relief Annealing" />
      <xs:enumeration value="Other" />
      <xs:enumeration value="None" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="quenchingType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="" />
      <xs:enumeration value="Ice Quenching" />
      <xs:enumeration value="Water Quenching" />
      <xs:enumeration value="Air Cooling" />
      <xs:enumeration value="Gas Cooling" />
      <xs:enumeration value="Brine Quenching" />
      <xs:enumeration value="Lqiuid Nitrogen Quenching" />
      <xs:enumeration value="Furnace Cooling" />
      <xs:enumeration value="Oil Quenching" />
    </xs:restriction>
  </xs:simpleType>


  <!-- PHYSICALPROPERTIES -->


  <xs:complexType name="LengthType">
    <xs:all>
      <xs:element name="lenghtQuanity" type="xs:float" />
      <xs:element name="lengthUnit" type="LengthUnit" />
      <xs:element minOccurs="0" name="uncertainty" type="Uncertainty" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="TemperatureType">
    <xs:sequence>
      <xs:element name="temperature" type="xs:float" />
      <xs:element name="unit" type="TemperatureUnit" />
      <xs:element minOccurs="0" name="uncertainty" type="Uncertainty" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TimeType">
    <xs:sequence>
      <xs:element name="duration" type="xs:double" />
      <xs:element name="unit" type="TimeUnit" />
      <xs:element minOccurs="0" name="uncertainty" type="Uncertainty" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Volume">
    <xs:sequence>
      <xs:element name="length" type="physical-quantity-type" minOccurs="1" maxOccurs="1" />
      <xs:element name="width" type="physical-quantity-type" minOccurs="1" maxOccurs="1" />
      <xs:element name="height" type="physical-quantity-type" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="parameter-type">
    <xs:complexContent>
      <xs:extension base="physical-quantity-type">
        <xs:sequence>
          <xs:element name="name" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="physical-quantity-type">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="value" type="xs:float" />
      <xs:element minOccurs="0" name="unit" type="xs:string" />
      <xs:element minOccurs="0" name="uncertainty" type="Uncertainty" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Range">
    <xs:sequence>
      <xs:element name="min" type="xs:float" minOccurs="1" maxOccurs="1" />
      <xs:element name="max" type="xs:float" minOccurs="1" maxOccurs="1" />
      <xs:element name="unit" type="xs:string" minOccurs="0" />
    </xs:sequence>
  </xs:complexType> 

  <xs:complexType name="Uncertainty">
    <xs:sequence>
      <xs:element name="type">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="amount" />
            <xs:enumeration value="fraction" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="value" type="xs:double" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="PhaseFractiontype">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Mole Fraction" />
      <xs:enumeration value="Volume Fraction" />
    </xs:restriction>
  </xs:simpleType>

  <!-- UTILITIES -->


  <xs:complexType name="Note">
    <xs:sequence>
      <xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="date" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
      <xs:element name="text" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="digitalArtifact" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="DigitalArtifact">
    <xs:sequence>
      <xs:element name="identifier" type="xs:string" minOccurs="0" />
      <xs:element minOccurs="0" name="title" type="xs:string" />
      <xs:element minOccurs="0" name="description" type="xs:string" />
      <xs:element minOccurs="0" name="type" type="DigitalArtifactType" />
      <xs:element minOccurs="0" name="format" type="xs:string" />
      <xs:element minOccurs="0" name="comment" type="xs:string" />
      <xs:element maxOccurs="unbounded" minOccurs="0" name="accessURL" type="xs:anyURI">
        <xs:annotation>
          <xs:documentation>URL providing direct access to a downloadable file of a dataset.
          </xs:documentation>
          <xs:appinfo>
            <module>module-advanced-blob-host</module>
            <label>Upload load a file that can then download using a direct URL</label>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
<!-- 
  <xs:complexType name="DigitalArtifacts">
    <xs:sequence>
      <xs:element name="digitalArtifact" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
 -->
  <xs:simpleType name="DigitalArtifactType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="file" />
      <xs:enumeration value="folder" />
      <xs:enumeration value="database" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="AMBlobReference">
    <xs:annotation>
      <xs:documentation>
        reference to an AMBlob document. Contains the handle and checksum of the actual blob.
        also informaiton about usage of the blob can be added here
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="handle" type="xs:anyURI" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            handle of the blob, also part of the AMDoc/AMBlob document.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="checksum" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            MD5 checksum of the referenced image.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            MD5 checksum of the referenced image.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Blob">
    <xs:sequence>
      <xs:element name="handle" type="xs:anyURI" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Handle assigned by CDCS to retrieve the blob.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="checksum" type="xs:string" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>MD5 checksum of the blob bytes. Must be provided to be able to check whether this image was
            already loaded.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cdcsPID" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="format" type="xs:string" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Table">
    <xs:sequence>
      <xs:element name="headers" type="Headers" />
      <xs:element name="rows" type="Rows" />
    </xs:sequence>
    <xs:attribute name="name" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="Headers">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="column" type="Column" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Rows">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="row" type="Row" />
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="Row">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="column" type="Column" />
    </xs:sequence>
    <xs:attribute name="id" type="xs:string" use="required" />
  </xs:complexType>

  <xs:complexType name="Column">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="id" type="xs:string" use="required" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="Person">
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="name" type="xs:string" />
      <xs:element maxOccurs="unbounded" minOccurs="0" name="email" type="xs:string" />
      <xs:element maxOccurs="1" minOccurs="0" name="orcid" type="xs:string" />
      <xs:element maxOccurs="unbounded" minOccurs="0" name="location" type="xs:string" />
      <xs:element maxOccurs="unbounded" minOccurs="0" name="phone" type="xs:string" />
      <xs:element maxOccurs="unbounded" minOccurs="0" name="affiliation" type="xs:string" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Contributor">
    <xs:sequence>
      <xs:element name="person" type="Person" minOccurs="1" />
      <xs:element name="role" type="xs:string" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:appinfo>
            <label>The role that a person plays. E.g. Primary Contact, Owner, Custodian, Responsible Party etc
            </label>
          </xs:appinfo>
        </xs:annotation>

      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Instrument">
    <!--  Instrument configuration, or usage, or setup. Intrument as used in an experiment -->
    <xs:complexContent>
      <xs:extension base="AMResource">
        <xs:sequence>
          <xs:element name="model" type="xs:string" minOccurs="0" maxOccurs="1" />
          <xs:element name="physicalLocation" type="xs:string" minOccurs="0" />
          <xs:element name="instrumentMetadata" type="ObjectType" minOccurs="0" maxOccurs="1" />
          <xs:element name="detector" type="Sensor" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="sensor" type="Sensor" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="supportingFile" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="Sensor">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="type" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="manufacturer" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="model" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="identifier" type="identifier" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="calibrationDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
      <xs:element name="range" type="Range" minOccurs="0" maxOccurs="1" />
      <xs:element name="accuracy" type="physical-quantity-type" minOccurs="0" maxOccurs="1" />
      <xs:element name="accuracyClass" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="specializedMetadata" type="ObjectType" minOccurs="0" maxOccurs="1" />      
    </xs:sequence>
  </xs:complexType>


  <!-- ================================== Chemical composition ================================== -->
  <xs:complexType name="Composition">
    <xs:sequence>
      <xs:element name="Constituents" type="Constituents" minOccurs="0" maxOccurs="1" />
      <xs:element name="quantityUnit" type="ConstituentQuantityUnit" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Constituents">
    <xs:annotation>
      <xs:appinfo>
        <module>module-chemical-composition-simple</module>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="constituent" type="ConstituentMaterial" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ConstituentMaterial">
    <xs:sequence>
      <xs:element name="element" type="ChemicalElement" minOccurs="1" maxOccurs="1" />
      <xs:element name="quantity" type="xs:double" minOccurs="1" maxOccurs="1" />
      <xs:element name="purity" type="xs:double" minOccurs="0" maxOccurs="1" />
      <xs:element name="error" type="xs:double" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
</xs:schema>