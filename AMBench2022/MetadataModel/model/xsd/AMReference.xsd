<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified">

  <!-- base types related to Resource-s, i.e. entities represented in an AMBench CDCS instance as document with PID assigned 
    etc. -->
  <xs:complexType name="AMResource" abstract="true">
    <xs:sequence>
      <xs:element name="name" minOccurs="1" maxOccurs="1" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Name assigned to this thing by the owner for example (e.g.
              AMBench2022-Material-Machine-Build#)
              Usable
              as an internal identifier.
            </label>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:appinfo>
            <label>Description of the resource </label>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <!--  extended measurement description documentation-->
      <xs:element name="documentation" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="primaryContact" type="Person" minOccurs="0" maxOccurs="1" />
      <xs:element name="contributor" type="Contributor" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="note" type="Note" minOccurs="0" maxOccurs="unbounded" />
      <xs:element maxOccurs="unbounded" minOccurs="0" name="altName" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Alternate Name (e.g. acronym, nickname)</label>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="UUID" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <label>
              A UUID can be used as an identifier and can for example be created when a first reference is made to
              an object,
              say in some spreadsheet.
              Can be generated online at https://www.uuidgenerator.net/version4.
              Should not be changed!
              Different from a PID (persistent id) in that the latter is often created when
              metadata is inserted in some database.
              TBD should this just be a special type of identifier?
            </label>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="identifier" type="identifier" maxOccurs="unbounded" minOccurs="0">
        <xs:annotation>
          <xs:appinfo>
            <label>Identifier-s different from PID and UUID</label>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="journalPublication" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="referencePublication" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded" />
      <!-- Standard name and link are mapped to title, and url -->      
      <xs:element name="relatedStandard" type="DigitalArtifact" minOccurs="0" maxOccurs="unbounded" />      
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="AMActivity" abstract="true">
    <xs:complexContent>
      <xs:extension base="AMResource">
        <xs:sequence>
          <xs:element name="startDate" minOccurs="0" maxOccurs="1" type="xs:dateTime" />
          <xs:element name="completeDate" minOccurs="0" maxOccurs="1" type="xs:dateTime" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="identifier">
    <xs:sequence>
      <xs:element name="id" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Identifier Value</label>
            <placeholder>E.g. http://dx.doi.org/123/abc;
              http://orcid.org/0000-0000-0000-0000
            </placeholder>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="type" type="xs:string">
        <xs:annotation>
          <xs:appinfo>
            <label>Identifier Type</label>
            <placeholder>E.g. DOI, Handle, ORCID</placeholder>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PIDReference">
    <xs:sequence>
      <xs:element name="documentType" type="ArtifactType"/>
      <xs:element name="pid" type="xs:anyURI" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Field">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="1" />
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="value" type="xs:string" />
        <xs:element name="quantity" type="physical-quantity-type" />
        <xs:element name="object" type="ObjectType" />
        <xs:element name="digitalArtifact" type="DigitalArtifact"/>
      </xs:choice>
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="documentationLink" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ObjectType">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1" />
      <xs:element name="field" type="Field" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="ArtifactType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="AMBuildPlate" />
      <xs:enumeration value="AMBuildPart" />
      <xs:enumeration value="AMBSpecimen" />
      <xs:enumeration value="AMPowder" />
      <xs:enumeration value="Material" />
    </xs:restriction>
  </xs:simpleType>

  <!-- UNITS -->
  <xs:simpleType name="TemperatureUnit">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Kelvin" />
      <xs:enumeration value="Celsius" />
      <xs:enumeration value="Fahrenheit" />
